    "composite": true,
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "../dist",
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "**/*.ts", "**/*.tsx"
  ]
}
